{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/WebApp/bin/Debug/net7.0/Sceny.Finance.WebApp.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/WebApp",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "DOTNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:5001"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "https://localhost:5001",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "/mnt/c/*": "C:/*"
      },
      "skipFiles": [
        "<node_internals>/**/*.js",
        "${workspaceFolder}/src/WebApp/ClientApp/node_modules/**/*.js"
      ]
    },
    {
      "type": "node-terminal",
      "name": "Run Script: dev",
      "request": "launch",
      "command": "npm run dev",
      "cwd": "${workspaceFolder}/src/WebApp/ClientApp"
    }
  ],
  "compounds": [
    {
      "name": "App Launch",
      "configurations": [
        ".NET Core Launch (web)",
        "Launch Chrome"
      ]
    }
  ]
}